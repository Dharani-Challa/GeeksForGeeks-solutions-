class Solution {
    static int searchInSorted(int arr[], int N, int K) {

        // Your code here
        int count=0;
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]==K)
                count++;
                
        }
        if(count!=0)
            return 1;
        else
            return -1;
    }
}
