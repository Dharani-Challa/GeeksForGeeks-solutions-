class Solution {
    public List<Integer> valueEqualToIndex(List<Integer> nums) {
        
        List<Integer> result=new ArrayList<Integer>();
        for(int i=0;i<nums.size();i++)
        {
            if(nums.get(i)==(i+1))
            {
                result.add(i+1);
            }
        }
        return result;
    }
}
